{
  "name": "react-github-graph",
  "version": "1.3.0",
  "description": "A lightweight React component for displaying a graph of Github contributions",
  "files": ["dist"],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "author": "Kyle Karpack <kylekarpack@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/kylekarpack/react-github-graph.git"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-controls": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-storysource": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "cross-env": "^7.0.2",
    "react": "^16.8.6",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "styled-components": "^4.3.1"
  },
  "peerDependencies": {
    "react": ">= 16",
    "styled-components": ">= 4"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepublish": "yarn build",
    "storybook": "cross-env NODE_PATH=src start-storybook -p 6006 --no-dll --docs",
    "build-storybook": "build-storybook --no-dll"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "@types/node": "^17.0.8",
    "@vitejs/plugin-react": "^1.1.4",
    "vite": "^2.7.10"
  }
}
