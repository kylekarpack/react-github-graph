{
  "name": "react-github-graph",
  "version": "1.5.2",
  "description": "A lightweight React component for displaying a graph of Github contributions",
  "files": [
    "dist"
  ],
  "main": "./dist/index",
  "typings": "./dist/index",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "author": "Kyle Karpack <kylekarpack@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/kylekarpack/react-github-graph.git"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-controls": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-storysource": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.13",
    "@types/node": "^17.0.8",
    "@types/styled-components": "^5.1.21",
    "@vitejs/plugin-react": "^2.2.0",
    "cross-env": "^7.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^17.0.1",
    "release-it": "^15.5.0",
    "styled-components": "^4.3.1",
    "vite": "^3.2.2",
    "vite-plugin-dts": "^1.6.6"
  },
  "peerDependencies": {
    "react": ">= 16",
    "styled-components": ">= 4"
  },
  "scripts": {
    "test": "echo 'No tests yet'",
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build",
    "release": "release-it"
  },
  "release-it": {
    "github": {
      "release": true,
      "web": true
    }
  }
}
